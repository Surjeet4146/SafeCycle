{
  "name": "safecycle",
  "version": "1.0.0",
  "description": "Safety-optimized cycling routes using Google Maps Platform",
  "scripts": {
    "start": "cd server && node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
